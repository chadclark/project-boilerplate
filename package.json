{
  "name": "project-boilerplate",
  "version": "4.0.0",
  "description": "A front-end boilerplate",
  "main": "index.js",
  "repository": "git@github.com:chadclark/project-boilerplate.git",
  "author": "Chad Clark <chad@chadjclark.com>",
  "license": "MIT",
  "private": true,
  "paths": {
    "src": {
      "base": "./src/",
      "fonts": "./fonts/",
      "img": "./img/",
      "js": "./js/",
      "scss": "./src/scss/"
    },
    "dist": {
      "base": "./public/assets/",
      "css": "./public/assets/css/",
      "img": "./public/assets/img/",
      "fonts": "./public/assets/fonts/",
      "js": "./public/assets/js/"
    },
    "templates": {
        "path": "./public/",
        "ext": ".html"
    }
  },
  "urls": {
    "live": "",
    "local": "http://projectboilerplate.dev/",
    "critical": "",
    "browserSync": "projectboilerplate.dev"
  },
  "vars": {
    "css": "main.css"
  },
  "dependencies": {
    "bourbon": "5.0.0-beta.7",
    "bourbon-neat": "^2.0.0",
    "fontfaceobserver": "^2.0.9",
    "lazysizes": "^3.0.0",
    "normalize.css": "^6.0.0",
    "picturefill": "^3.0.2",
    "svg4everybody": "^2.1.7"
  },
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "critical": "^0.8.4",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^3.2.0",
    "gulp-load-plugins": "^1.3.0",
    "gulp-notify": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.2.1"
  }
}
